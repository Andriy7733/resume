<footer>
    <div class="nav nav-tabs my-5">
        {{#each footer}}
        <div class="nav-item">
            {{#each this}}
            <a href="{{this.link}}" class="nav-link {{#if @last}}disabled{{/if}}">{{this.text}}</a>
            {{/each}}
        </div>
        {{/each}}
    </div>
</footer>