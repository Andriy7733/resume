<div class="row">
    {{#with navigation}}

    <div class="col-8">
        <div>{{Logo}}</div>
    </div>

    <div class="col-4">
        <div class="nav">
            {{#each links}}
            <div class="nav-item">
                <a href="{{href}}" class="nav-link">{{text}}</a>
            </div>
            {{/each}}
        </div>
    </div>
    {{/with}}
</div>